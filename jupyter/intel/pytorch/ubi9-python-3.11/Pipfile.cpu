[[source]]
url = "https://pypi.org/simple"
verify_ssl = true
name = "pypi"

[[source]]
url = "https://download.pytorch.org/whl/cpu"
verify_ssl = true
name = "torch-cpu"

[[source]]
url = "https://pytorch-extension.intel.com/release-whl/stable/cpu/us/"
verify_ssl = true
name = "ipex-cpu"

[packages]
# PyTorch packages
torch = {version="==2.1.2", index="torch-cpu"}
intel-extension-for-pytorch = {version="==2.1.100", index="ipex-cpu"}
torchvision = {version="0.16.2", index="torch-cpu"}
torchaudio = {version="2.1.2", index="torch-cpu"}
neural-compressor = {version="2.4.1"}
tensorboard = {version="*"}
# LLM Packages
datasets = {version="*"}
transformers = {version="*"}
evaluate = {version="*"}
# Datascience and useful extensions
kafka-python = "~=2.0.2"
matplotlib = "~=3.6.3"
pandas = "~=1.5.3"
plotly = "~=5.16.1"
scikit-learn = "*"
scipy = "~=1.11.2"
skl2onnx = "~=1.15.0"
codeflare-sdk = "~=0.23.0"
# DB connectors
pymongo = "~=4.5.0"
psycopg = "~=3.1.10"
pyodbc = "~=4.0.39"
mysql-connector-python = "~=8.0.33"
# JupyterLab packages
elyra-pipeline-editor-extension = "~=3.15.0"
elyra-python-editor-extension = "~=3.15.0"
elyra-code-snippet-extension = "~=3.15.0"
kfp-tekton = "<1.6.0"
jupyterlab = "~=3.6.5"
jupyter-bokeh = "~=3.0.7"
jupyter-server = "~=2.7.3"
jupyter-server-proxy = "~=4.0.0"
jupyter-server-terminals = "~=0.4.4"
jupyterlab-git = "~=0.42.0"
jupyterlab-lsp = "~=4.2.0"
jupyterlab-widgets = "~=3.0.8"
jupyter-resource-usage = "~=1.0.1"
nbdime = "~=3.2.1"
nbgitpuller = "~=1.2.0"

[requires]
python_version = "3.11"
